{
	"name": "pxserver-frontend",
	"version": "1.0.0",
	"description": "pxserver",
	"author": "etienne-k",
	"license": "GPL-3.0-or-later",
	"main": "run/start.js",
	"scripts": {
		"ng": "ng",
		"start": "node .",
		"build": "yarn run build-server && yarn run build-client && yarn run copy-assets",
		"watch": "concurrently -n \"ts,nd\" \"tsc --pretty -w\" \"nodemon .\"",
		"build-server": "tsc --pretty -p ./tsconfig.server.json",
		"build-client": "ng build",
		"copy-assets": "ts-node -T --project ./tsconfig.server.json copy-assets.ts",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e"
	},
	"dependencies": {
		"@angular/animations": "~8.2.0",
		"@angular/common": "~8.2.0",
		"@angular/compiler": "~8.2.0",
		"@angular/core": "~8.2.0",
		"@angular/forms": "~8.2.0",
		"@angular/platform-browser": "~8.2.0",
		"@angular/platform-browser-dynamic": "~8.2.0",
		"@angular/router": "~8.2.0",
		"@types/redis": "^2.8.13",
		"express": "~4.17.1",
		"redis": "^2.8.0",
		"rxjs": "~6.4.0",
		"socket.io-client": "~2.2.0",
		"tslib": "^1.10.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.802.2",
		"@angular/cli": "~8.2.2",
		"@angular/compiler-cli": "~8.2.0",
		"@angular/language-service": "~8.2.0",
		"@types/express": "^4.16.1",
		"@types/node": "~8.9.4",
		"@types/shelljs": "^0.8.5",
		"@types/socket.io-client": "~1.4.32",
		"concurrently": "^5.0.0",
		"nodemon": "^1.19.4",
		"shelljs": "~0.8.3",
		"ts-node": "~7.0.0",
		"tslint": "~5.15.0",
		"typescript": "~3.5.3"
	}
}
